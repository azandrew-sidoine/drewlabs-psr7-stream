{
    "name": "drewlabs/psr7-stream",
    "description": "Provides utility classes and implementations for the PSR7 stream interface",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Drewlabs\\Psr7Stream\\": "src/"
        },
        "files": [
            "./helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Drewlabs\\Psr7Stream\\Tests\\": "tests/"
        },
        "classmap": [
            "tests/"
        ]
    },
    "authors": [
        {
            "name": "azandrew-sidoine",
            "email": "asmyns.platonnas29@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0",
        "psr/http-message": "^1.0",
        "psr/http-factory": "^1.0"
    }
}
